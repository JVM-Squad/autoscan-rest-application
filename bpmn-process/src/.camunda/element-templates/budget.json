[
  {
    "$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
    "name": "Budget Lookup",
    "id": "com.jongsoft.finance.bpmn.budget.BudgetLookup",
    "description": "Locate an budget item based upon its name.",
    "appliesTo": [
      "bpmn:ServiceTask"
    ],
    "properties": [
      {
        "label": "Implementation",
        "type": "String",
        "value": "${processBudgetLookupDelegate}",
        "editable": false,
        "binding": {
          "type": "property",
          "name": "camunda:delegateExpression"
        }
      },
      {
        "label": "Name",
        "type": "String",
        "description": "The name of the budget item to locate.",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "name"
        },
        "constraint": {
          "notEmpty": true
        }
      },
      {
        "label": "Resolved budget",
        "type": "String",
        "description": "The budget item that was found.",
        "binding": {
          "type": "camunda:outputParameter",
          "source": "${budget}"
        }
      }
    ]
  },
  {
    "$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
    "name": "Budget Create",
    "id": "com.jongsoft.finance.bpmn.budget.BudgetCreate",
    "appliesTo": [
      "bpmn:ServiceTask"
    ],
    "properties": [
      {
        "label": "Implementation",
        "type": "String",
        "value": "${processBudgetCreateDelegate}",
        "editable": false,
        "binding": {
          "type": "property",
          "name": "camunda:delegateExpression"
        }
      },
      {
        "label": "Budget JSON",
        "type": "String",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "budget"
        },
        "constraint": {
          "notEmpty": true
        }
      }
    ]
  }
]
